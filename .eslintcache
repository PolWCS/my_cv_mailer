ocess.env.REACT_APP_HOST, httpClient);\nconst history = createHashHistory();\n\nconst Home = () => {\n  // const [routes, setRoutes] = useState([]);\n\n  // useEffect(() => {\n  //   if (user && user.role === \"super_admin\") {\n  //     setRoutes(superAdminRoutes);\n  //   }\n  // }, [user]);\n\n  return (\n    <>\n      <Admin\n        // layout={MyLayout}\n        theme={theme}\n        dashboard={Dashboard}\n        dataProvider={dataProvider}\n        authProvider={authProvider}\n        history={history}\n      >\n        <Resource\n          name=\"users\"\n          list={UserList}\n          show={UserShow}\n          edit={UserEdit}\n        />\n        <Resource\n          name=\"curriculum_vitae\"\n          list={CvList}\n          show={CvShow}\n          edit={CvEdit}\n          create={CvCreate}\n        />\n        <Resource\n          name=\"motivation_mails\"\n          list={MotivMailList}\n          show={MotivMailShow}\n          edit={MotivMailEdit}\n          create={MotivMailCreate}\n        />\n        <Resource\n          name=\"contact_book\"\n          list={ContactBookList}\n          show={ContactBookShow}\n          edit={ContactBookEdit}\n          create={ContactBookCreate}\n        />\n        <Resource name=\"history\" list={HistoryList} show={HistoryShow} />\n      </Admin>\n    </>\n  );\n};\n\nexport default connect()(Home);\n","/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/login/authProvider.jsx",["244"],"import axios from \"axios\";\n\nexport default {\n  // called when the user attempts to log in\n  login: async ({ username, password }) => {\n    await axios\n      .post(`${process.env.REACT_APP_HOST}/users/signin`, {\n        email: username,\n        password,\n      })\n      .then((res) => res.data)\n      .then((cred) => {\n        sessionStorage.setItem(\"auth\", JSON.stringify(cred));\n        return Promise.resolve();\n      })\n      .catch(() => Promise.reject());\n    // return Promise.resolve();\n  },\n  // called when the user clicks on the logout button\n  logout: () => {\n    sessionStorage.removeItem(\"auth\");\n    return Promise.resolve();\n  },\n  // called when the API returns an error\n  checkError: ({ status }) => {\n    if (status === 401 || status === 403) {\n      sessionStorage.removeItem(\"auth\");\n      return Promise.reject();\n    }\n    return Promise.resolve();\n  },\n  // called when the user navigates to a new location, to check for authentication\n  checkAuth: () => {\n    return sessionStorage.getItem(\"auth\")\n      ? Promise.resolve()\n      : Promise.reject();\n  },\n  getIdentity: () => {\n    const auth = JSON.parse(sessionStorage.getItem(\"auth\"));\n    return auth\n      ? Promise.resolve({ id: auth.id, fullName: auth.fullName })\n      : Promise.reject();\n  },\n  getPermissions: () => {\n    // const auth = JSON.parse(sessionStorage.getItem(\"auth\"));\n    return Promise.resolve();\n    // return auth ? Promise.resolve(auth.role) : Promise.reject();\n  },\n};\n","/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/curriculumVitae/CvList.jsx",["245"],"import React from \"react\";\nimport { List, Datagrid, TextField, EditButton, UrlField } from \"react-admin\";\n\nconst CvList = (props) => (\n  <List {...props}>\n    <Datagrid rowClick=\"show\">\n      <TextField source=\"title\" />\n      {/* <UrlField source=\"url\" /> */}\n      <EditButton />\n    </Datagrid>\n  </List>\n);\n\nexport default CvList;\n","/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/contactBook/ContactBookList.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/history/HistoryList.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/motivationMail/MotivMailList.jsx",["246"],"import React from \"react\";\nimport { List, Datagrid, TextField, EditButton, UrlField } from \"react-admin\";\n\nconst MotivMailList = (props) => (\n  <List {...props}>\n    <Datagrid rowClick=\"show\">\n      <TextField source=\"title\" />\n      {/* <UrlField source=\"url\" /> */}\n      <EditButton />\n    </Datagrid>\n  </List>\n);\n\nexport default MotivMailList;\n","/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/curriculumVitae/CvShow.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/motivationMail/MotivMailShow.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/history/HistoryShow.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/contactBook/ContactBookShow.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/curriculumVitae/CvEdit.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/contactBook/ContactBookEdit.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/motivationMail/MotivMailEdit.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/user/UserEdit.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/user/UserList.jsx",["247"],"import React from \"react\";\nimport { List, Datagrid, TextField, EmailField, EditButton } from \"react-admin\";\n\nconst UserList = (props) => (\n  <List {...props}>\n    <Datagrid rowClick=\"show\">\n      <EmailField source=\"email\" />\n      <EditButton />\n    </Datagrid>\n  </List>\n);\n\nexport default UserList;\n","/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/user/UserShow.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/motivationMail/form/MotivMailForm.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/curriculumVitae/form/CvForm.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/contactBook/form/ContactBookForm.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/motivationMail/MotivMailCreate.jsx",["248"],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/curriculumVitae/CvCreate.jsx",["249"],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/contactBook/ContactBookCreate.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/actions/index.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/dashboard/Dashboard.jsx",["250"],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/dashboard/mailer/Mailer.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/dashboard/mailer/form/MailerForm.jsx",["251","252"],"import React, { useEffect, useState } from \"react\";\nimport { connect } from \"react-redux\";\n\nimport { setEmailFormDataAction } from \"../../../../redux/actions/emailActions\";\nimport { getCvIdAction } from \"../../../../redux/actions/cvActions\";\nimport { getMMIdAction } from \"../../../../redux/actions/motivMailActions\";\nimport { getCurrentContactIdAction } from \"../../../../redux/actions/contactBookActions\";\n\nimport \"../../../../css/dashboard/mailer/form/MailerForm.css\";\n\nconst MailerForm = ({\n  emailFormData,\n  setEmailFormData,\n  motivMailList,\n  cvList,\n  user,\n  contactBookList,\n  getMMId,\n  getCvId,\n  getCurrentContactId,\n}) => {\n  const [select, setSelect] = useState({\n    cvUrl: null,\n    mmUrl: null,\n    message: \"\",\n  });\n  const handleInput = (event) => {\n    setEmailFormData({\n      ...emailFormData,\n      [event.target.name]: event.target.value,\n    });\n    if (event.target.name === \"message\") {\n      setSelect({ ...select, message: event.target.value });\n    }\n  };\n\n  useEffect(() => {\n    if (emailFormData.to) getCurrentContactId(emailFormData.to);\n  }, [emailFormData.to]);\n\n  useEffect(() => {\n    setEmailFormData({\n      ...emailFormData,\n      html: `<div><p>${select.message}</p><p>CV ${user && user.fullName} ${\n        select.cvUrl\n      }</p><p>Motivation ${user && user.fullName} ${select.mmUrl}</p></div>`,\n      attachments: [\n        { filename: `CV ${user && user.fullName}`, path: select.cvUrl },\n        { filename: `Motivation ${user && user.fullName}`, path: select.mmUrl },\n      ],\n    });\n    if (select.cvUrl) {\n      getCvId(select.cvUrl);\n    }\n    if (select.mmUrl) {\n      getMMId(select.mmUrl);\n    }\n  }, [select]);\n\n  return (\n    <form className=\"mailer_form\">\n      <div className=\"mailer_form_select_container\">\n        <label htmlFor=\"to\" className=\"mailer_form_label\">\n          <select\n            name=\"to\"\n            className=\"mailer_form_input\"\n            value={emailFormData.to}\n            onChange={handleInput}\n          >\n            <option value=\"\">--Sélectionnez un contact--</option>\n            {contactBookList.length > 0 &&\n              contactBookList.map((contact) => (\n                <option value={contact.email}>\n                  {contact.firm} {contact.email} {contact.lastname}{\" \"}\n                  {contact.firstname}{\" \"}\n                </option>\n              ))}\n          </select>\n        </label>\n      </div>\n      <div className=\"mailer_form_mail_content_container\">\n        <label htmlFor=\"subject\" className=\"mailer_form_label\">\n          <input\n            name=\"subject\"\n            className=\"mailer_form_input\"\n            value={emailFormData.subject}\n            placeholder=\"Objet\"\n            onChange={handleInput}\n          />\n        </label>\n        <label htmlFor=\"message\" className=\"mailer_form_label\">\n          <textarea\n            name=\"message\"\n            className=\"mailer_form_textarea\"\n            placeholder=\"Tapez votre message ici...\"\n            value={emailFormData.message}\n            onChange={handleInput}\n          />\n        </label>\n      </div>\n      <div className=\"mailer_form_select_container\">\n        <label htmlFor=\"cvUrl\" className=\"mailer_form_label\">\n          <select\n            name=\"cvUrl\"\n            className=\"mailer_form_input\"\n            value={select.cvUrl}\n            onChange={(event) =>\n              setSelect({ ...select, cvUrl: event.target.value })\n            }\n          >\n            <option value=\"\">--Sélectionnez un CV--</option>\n            {cvList.length > 0 &&\n              cvList.map((cv) => <option value={cv.url}>{cv.title}</option>)}\n          </select>\n        </label>\n        <label htmlFor=\"mmUrl\" className=\"mailer_form_label\">\n          <select\n            name=\"mmUrl\"\n            className=\"mailer_form_input\"\n            value={select.mmUrl}\n            onChange={(event) =>\n              setSelect({ ...select, mmUrl: event.target.value })\n            }\n          >\n            <option value=\"\">--Sélectionnez une lettre de motivation--</option>\n            {motivMailList.length > 0 &&\n              motivMailList.map((mail) => (\n                <option value={mail.url}>{mail.title}</option>\n              ))}\n          </select>\n        </label>\n      </div>\n    </form>\n  );\n};\n\nconst mapStateToProps = ({\n  custom: { emailFormData, motivMailList, cvList, user, contactBookList },\n}) => ({\n  emailFormData,\n  motivMailList,\n  cvList,\n  user,\n  contactBookList,\n});\n\nconst mapDispatchToProps = (dispatch) => ({\n  setEmailFormData: setEmailFormDataAction(dispatch),\n  getMMId: getMMIdAction(dispatch),\n  getCvId: getCvIdAction(dispatch),\n  getCurrentContactId: getCurrentContactIdAction(dispatch),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(MailerForm);\n","/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/reducers/cvReducers.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/reducers/motivMailReducers.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/reducers/contactBookReducers.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/actions/cvActions.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/actions/motivMailActions.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/actions/contactBookActions.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/reducers/emailReducers.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/actions/emailActions.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/dashboard/mailer/buttons/SendMailBtn.jsx",["253"],"import React from \"react\";\nimport axios from \"axios\";\nimport { connect } from \"react-redux\";\nimport Button from \"@material-ui/core/Button\";\n\nimport { setToggleAction } from \"../../../../redux/actions/toggleActions\";\nimport { setEmailFormDataAction } from \"../../../../redux/actions/emailActions\";\n\nconst SendMailBtn = ({\n  emailFormData,\n  currentMMail,\n  currentCv,\n  user,\n  currentContact,\n  setToggle,\n  setEmailFormData,\n}) => {\n  const handleSubmit = async () => {\n    await axios.post(`${process.env.REACT_APP_HOST}/email/send`, {\n      to: emailFormData.to,\n      subject: emailFormData.subject,\n      message: emailFormData.message,\n      html: emailFormData.html,\n      attachments: emailFormData.attachments,\n    });\n    await axios.post(`${process.env.REACT_APP_HOST}/history`, {\n      subject: emailFormData.subject,\n      message: emailFormData.message,\n      date: new Date()\n        .toLocaleDateString()\n        .split(\"/\")\n        .reverse()\n        .map((e, i, w) => {\n          if (i === 0) {\n            return e;\n          } else if (i === 1) {\n            return w[2];\n          } else if (i === 2) {\n            return w[1];\n          }\n        })\n        .join(\"-\"),\n      user_id: user.id,\n      contact_book_id: currentContact.id,\n      cv_id: currentCv.id,\n      mm_id: currentMMail.id,\n      reply: 0,\n    });\n    setToggle();\n    setEmailFormData();\n  };\n\n  return (\n    <Button\n      variant=\"contained\"\n      color=\"primary\"\n      style={{ margin: \"5px\", padding: \"1em\" }}\n      onClick={handleSubmit}\n    >\n      Envoyer\n    </Button>\n  );\n};\n\nconst mapStateToProps = ({\n  custom: { emailFormData, currentMMail, currentCv, user, currentContact },\n}) => ({ emailFormData, currentMMail, currentCv, user, currentContact });\n\nconst mapDispatchToProps = (dispatch) => ({\n  setToggle: setToggleAction(dispatch),\n  setEmailFormData: setEmailFormDataAction(dispatch),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(SendMailBtn);\n","/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/utils/index.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/base.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/reducers/currentDoc.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/actions/docActions.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/_reusable/DropZone.jsx",["254"],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/reducers/alertReducers.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/motivationMail/buttons/NewMMailSave.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/actions/alertActions.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/curriculumVitae/buttons/NewCvSaveBtn.jsx",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/reducers/historyReducers.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/components/dashboard/replies/Recall.jsx",["255","256","257","258"],"import React, { useEffect, useState } from \"react\";\nimport { connect } from \"react-redux\";\nimport axios from \"axios\";\nimport Button from \"@material-ui/core/Button\";\n\nimport { getRecallListAction } from \"../../../redux/actions/historyActions\";\n\nimport \"../../../css/dashboard/replies/Recall.css\";\n\nconst Recall = ({ user, getRecallList, recallList }) => {\n  const [message, setMessage] = useState({ show: false, text: \"\" });\n\n  useEffect(() => {\n    getRecallList();\n  }, []);\n\n  const handleRecall = async (id) => {\n    const currentRecall = recallList.filter((recall) => +recall.id === +id);\n    console.log(currentRecall[0]);\n    const response1 = await axios.get(\n      `${process.env.REACT_APP_HOST}/contact_book/${currentRecall[0].contact_book_id}`\n    );\n    const data1 = response1.data;\n    const response2 = await axios.get(\n      `${process.env.REACT_APP_HOST}/curriculum_vitae/${currentRecall[0].cv_id}`\n    );\n    const data2 = response2.data;\n    const response3 = await axios.get(\n      `${process.env.REACT_APP_HOST}/motivation_mails/${currentRecall[0].mm_id}`\n    );\n    const data3 = response3.data;\n    await axios.post(`${process.env.REACT_APP_HOST}/email/send`, {\n      to: data1.email,\n      message: currentRecall[0].message,\n      subject: currentRecall[0].subject,\n      html: `<div><p>${currentRecall[0].message}</p><p>CV ${\n        user && user.fullName\n      } ${data2.url}</p><p>Motivation ${user && user.fullName} ${\n        data3.url\n      }</p></div>`,\n      attachments: [\n        { filename: data2.title, path: data2.url },\n        { filename: data3.title, path: data3.url },\n      ],\n    });\n    await axios.put(\n      `${process.env.REACT_APP_HOST}/history/${currentRecall[0].id}`,\n      {\n        date: new Date()\n          .toLocaleDateString()\n          .split(\"/\")\n          .reverse()\n          .map((e, i, w) => {\n            if (i === 0) {\n              return e;\n            } else if (i === 1) {\n              return w[2];\n            } else if (i === 2) {\n              return w[1];\n            }\n          })\n          .join(\"-\"),\n      }\n    );\n    await getRecallList();\n  };\n\n  const handleCheck = async (id) => {\n    await axios.put(`${process.env.REACT_APP_HOST}/history/${id}`, {\n      reply: 1,\n    });\n    await getRecallList();\n  };\n\n  const handleMessage = (mess) => {\n    setMessage({\n      show: mess === message.text ? !message.show : true,\n      text: mess,\n    });\n  };\n\n  return (\n    <div className=\"recall_container\">\n      <div className=\"recall_list_container\">\n        {recallList.length > 0 ? (\n          recallList.map((e) => {\n            return (\n              <div className=\"recall_list_sub_container\">\n                <div className=\"recall_list_item_container\">\n                  <div className=\"recall_contact_container\">\n                    <p>{e.contact_book_firm}</p>\n                    <p>{e.contact_book_email}</p>\n                    <p>{e.subject}</p>\n                  </div>\n                  <div className=\"recall_message_btn_container\">\n                    <Button\n                      variant=\"outlined\"\n                      color=\"secondary\"\n                      onClick={() => handleMessage(e.message)}\n                    >\n                      Message\n                    </Button>\n                  </div>\n                  <div className=\"recall_link_container\">\n                    <a className=\"recall_link\" href={e.cv_url} target=\"_blank\">\n                      CV\n                    </a>{\" \"}\n                    <a className=\"recall_link\" href={e.mm_url} target=\"_blank\">\n                      MM\n                    </a>\n                  </div>\n                </div>\n                <div className=\"recall_list_btn_container\">\n                  <Button\n                    variant=\"text\"\n                    color=\"secondary\"\n                    onClick={() => handleRecall(e.id)}\n                  >\n                    Renvoyer\n                  </Button>\n                  <Button\n                    variant=\"text\"\n                    color=\"secondary\"\n                    onClick={() => handleCheck(e.id)}\n                  >\n                    Valider\n                  </Button>\n                </div>\n              </div>\n            );\n          })\n        ) : (\n          <p>No data</p>\n        )}\n      </div>\n      <div className=\"recall_msg_container\">{message.show && message.text}</div>\n    </div>\n  );\n};\n\nconst mapStateToProps = ({\n  custom: { recallList, contactBookList, motivMailList, cvList, user },\n}) => ({ recallList, contactBookList, motivMailList, cvList, user });\n\nconst mapDispatchToProps = (dispatch) => ({\n  getRecallList: getRecallListAction(dispatch),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Recall);\n","/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/actions/historyActions.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/reducers/toggleReducer.js",[],"/home/pol/Train/Checkpoint/checkpoint_4/my_cv_mailer/src/redux/actions/toggleActions.js",[],{"ruleId":"259","replacedBy":"260"},{"ruleId":"261","replacedBy":"262"},{"ruleId":"263","severity":1,"message":"264","line":9,"column":1,"nodeType":"265","endLine":56,"endColumn":3},{"ruleId":"266","severity":1,"message":"267","line":40,"column":6,"nodeType":"268","endLine":40,"endColumn":22,"suggestions":"269"},{"ruleId":"266","severity":1,"message":"270","line":51,"column":6,"nodeType":"268","endLine":51,"endColumn":12,"suggestions":"271"},{"ruleId":"266","severity":1,"message":"272","line":59,"column":6,"nodeType":"268","endLine":59,"endColumn":16,"suggestions":"273"},{"ruleId":"274","severity":1,"message":"275","line":1,"column":17,"nodeType":"276","messageId":"277","endLine":1,"endColumn":26},{"ruleId":"274","severity":1,"message":"278","line":1,"column":28,"nodeType":"276","messageId":"277","endLine":1,"endColumn":36},{"ruleId":"274","severity":1,"message":"279","line":3,"column":8,"nodeType":"276","messageId":"277","endLine":3,"endColumn":17},{"ruleId":"274","severity":1,"message":"280","line":9,"column":10,"nodeType":"276","messageId":"277","endLine":9,"endColumn":14},{"ruleId":"274","severity":1,"message":"281","line":9,"co